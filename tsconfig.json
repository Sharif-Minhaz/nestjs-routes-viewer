	"compilerOptions": {
		"target": "es2016",
		"module": "commonjs",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"baseUrl": "./",
		"paths": {
			"@nestjs/routes-viewer": ["src"],
			"@nestjs/routes-viewer/*": ["src/*"]
		}
	}
}
